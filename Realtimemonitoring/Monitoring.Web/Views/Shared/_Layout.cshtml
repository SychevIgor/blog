    <script type="text/javascript" src="~/Scripts/TS/DiagramViewModel.js"></script>
    <script type="text/javascript" src="~/Scripts/TS/Pages/BasePageModel.js"></script>
    
    <script type="text/javascript">

        var onDeleteDiagramClick = function (id) {

            var diagramId = id.replace("button", "");

            model.close(diagramId);
        };

        var onChangeDiagramTypeClick = function (id) {

            var diagramId = id.replace("button", "");

            model.changeDiagramType(diagramId);
        };

        var onChangeDataCollectingClick = function (id) {

            var diagramId = id.replace("button", "");

            model.onChangeDataCollectingClick(diagramId);
        };
    </script>
    @RenderBody()
        </head>
        <body>
        </body>
</html>